<?xml version="1.0" encoding="utf-8"?>
<com.job.androidtv.views.TvRelativeLayoutAsGroup xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tv="http://schemas.android.com/apk/res-auto"
    android:clipChildren="false"
    android:clipToPadding="false"
    tv:cursorRes="@drawable/cursor_rectangle_boarder_orange"
    tv:boarder="@dimen/px10"
    tv:scalable="false"
    android:paddingTop="@dimen/px20"
    android:paddingLeft="@dimen/px20"
    android:paddingRight="@dimen/px20"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <com.job.androidtv.views.ModuleWithImageText
        android:id="@+id/job_recommend"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="true"
        />
    <com.job.androidtv.views.ModuleWithImageText
        android:id="@+id/job_search"
        android:layout_toRightOf="@id/job_recommend"
        android:layout_marginBottom="@dimen/px20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="true"
        />
    <com.job.androidtv.views.ModuleWithImageText
        android:id="@+id/salary_query"
        android:layout_below="@id/job_search"
        android:layout_toRightOf="@id/job_recommend"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="true"
        />
    <com.job.androidtv.views.ModuleWithImageText
        android:id="@+id/who_see_my_resume"
        android:layout_toRightOf="@id/job_search"
        android:layout_marginBottom="@dimen/px20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="true"
        />
    <com.job.androidtv.views.ModuleWithImageText
        android:id="@+id/login"
        android:layout_below="@id/who_see_my_resume"
        android:layout_toRightOf="@id/salary_query"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="true"
        />
    <com.job.androidtv.views.ModuleWithImageText
        android:id="@+id/job_ask_record"
        android:layout_toRightOf="@id/who_see_my_resume"
        android:layout_marginBottom="@dimen/px20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="true"
        />
    <com.job.androidtv.views.ModuleWithImageText
        android:id="@+id/job_collect"
        android:layout_below="@id/job_ask_record"
        android:layout_toRightOf="@id/login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusableInTouchMode="true"
        />
</com.job.androidtv.views.TvRelativeLayoutAsGroup>